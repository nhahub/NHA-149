# Environment Variables for Taqyeem Frontend

# API Configuration
# For production: Use your Vercel backend URL (e.g., https://taqyeem-backend.vercel.app/api/v1)
# For development: Use local backend URL
VITE_API_BASE_URL=http://localhost:5000/api/v1

# App Configuration
VITE_APP_NAME=Taqyeem
VITE_APP_NAME_AR=تقييم
VITE_APP_DESCRIPTION=Bilingual Interview & Learning Platform
VITE_APP_DESCRIPTION_AR=منصة المقابلات والتعلم ثنائية اللغة

# Environment
VITE_NODE_ENV=development

# Features
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_ANALYTICS=false
